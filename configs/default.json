{
	"servers": [
		{
			"port": 8002,
			"host": "172.21.227.73",
			"server_name": "localhost",
			"dataDirectory": "./public",
			"root": "docs/fusion_web/",
			"index": "index.html",
			"client_max_size_body": 204800,
			"error_pages": [
				{
					"codes": "405",
					"page": "/AllErrorPages/40x.html"
				}
			],
			"routes": [
				{
					"routePath": "/poster",
					"savePath": "/posting",
					"allow_methods": "GET POST DELETE"
				},
				{
					"routePath": "/posting/",
					"allow_methods": "DELETE"
				},
				{
					"routePath": "/",
					"autoindex": false,
					"allow_methods": "GET"
				},
				{
					"routePath": "/AllErrorPages/",
					"allow_methods": "GET"
				},
				{
					"routePath": "/assets/",
					"allow_methods": "GET"
				},
				{
					"routePath": "/characters",
					"routeIndex": "characters.html",
					"autoindex": true,
					"allow_methods": "GET POST PUT HEAD"
				},
				{
					"routePath": "/theme",
					"index": "ThemeSwitcher/index.html",
					"autoindex": true,
					"allow_methods": "GET POST PUT HEAD"
				},
				{
					"routePath": "/ThemeSwitcher/",
					"allow_methods": "GET"
				},
				{
					"routePath": "/envestora",
					"index": "Envestora/index.html",
					"autoindex": true,
					"allow_methods": "GET POST PUT HEAD"
				},
				{
					"routePath": "/Envestora/",
					"allow_methods": "GET"
				},
				{
					"routePath": "/char",
					"redirect": "/characters",
					"allow_methods": "GET"
				},
				{
					"routePath": "/guru",
					"redirect": "https://www.google.com/",
					"allow_methods": "GET"
				},
				{
					"routePath": "/cgi-bin",
					"index": "dice.py",
					"root": "./",
					"cgi": [
						{
							"extension": ".py",
							"path": "/usr/bin/python3"
						},
						{
							"extension": ".sh",
							"path": "/bin/bash"
						}
					],
					"allow_methods": "GET POST DELETE"
				}
			]
		},
		{
			"port": 8000,
			"host": "127.0.0.1",
			"server_name": "Second Server"
		}
	]
}