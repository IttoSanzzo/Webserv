{
	"servers": [
		{
			"port": 8002,
			"host": "127.0.0.1",
			"server_name": "localhost",
			"root": "docs/fusion_web/",
			"index": "index.html",
			"client_max_size_body": 2048,
			"error_pages": [
				{
					"codes": "403",
					"page": "error_pages/403.html"
				},
				{
					"codes": "404",
					"page": "error_pages/404.html"
				},
				{
					"codes": "405",
					"page": "error_pages/40x.html"
				}
			],
			"routes": [
				{
					"routePath": "/",
					"autoindex": false,
					"allow_methods": "GET POST DELETE"
				},
				{
					"routePath": "/characters",
					"routeIndex": "characters.html",
					"autoindex": true,
					"allow_methods": "GET POST PUT HEAD"
				},
				{
					"routePath": "/theme",
					"index": "ThemeSwitcher/index.html",
					"autoindex": true,
					"allow_methods": "GET POST PUT HEAD"
				},
				{
					"routePath": "/char",
					"return": "/characters"
				},
				{
					"routePath": "/cgi-bin",
					"index": "dice.py",
					"root": "./",
					"cgi": [
						{
							"extension": ".py",
							"path": "/usr/bin/python3"
						},
						{
							"extension": ".sh",
							"path": "/bin/bash"
						}
					],
					"allow_methods": "GET POST DELETE"
				}
			]
		},
		{
			"port": 8000,
			"host": "127.0.0.1",
			"server_name": "Second Server"
		}
	]
}